CREATE TABLE AddToCart (
	id	INTEGER PRIMARY KEY AUTOINCREMENT,
	CategoryID	INTEGER,
	ProductID	INTEGER,
	Name	TEXT,
	UnitType	TEXT,
	UnitTypes	TEXT,
	UnitValue	REAL,
	KgWeight	REAL
);

select_all_data:
SELECT * FROM AddToCart;

select_all_data_by_id:
SELECT * FROM AddToCart WHERE CategoryID = ? AND ProductID = ?;